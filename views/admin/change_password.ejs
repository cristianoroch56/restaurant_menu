<body>
<!-- Preloader -->
<div class="preloader">
  <div class="cssload-speeding-wheel"></div>
</div>
<section id="wrapper" class="login-register">
  <div class="login-box ">
      <% if (message != '') { %>
        <div class="alert alert-danger col-sm-12">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
          <strong><%= message %></strong>
        </div>
      <% } %>
    <div class="white-box">
        <form class="form-horizontal form-material" data-toggle="validator"  id="loginform" class="form" action="<%= siteURL %>update_password" method="POST"> 
            <div class="form-group ">
                <div class="col-xs-12">
                  <h3>Change Password</h3>
                  <p class="text-muted">It's a good idea to use a strong password that you don't use elsewhere! </p>
                </div>
              </div>
           
              <div class="form-group ">
                <div class="col-xs-12">
                  <input type="hidden" name="id" value="1">
                  <input class="form-control" type="password" name="old_password" required="" placeholder="Current">
                </div>
              </div>
              <div class="form-group ">
                <div class="col-xs-12">
                  <input class="form-control" type="password" name="password" required="" placeholder="New">
                </div>
              </div>

              <div class="form-group text-center m-t-20">
                <div class="col-xs-12">
                  <button class="btn btn-primary btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Reset</button>
                  
                </div>
              </div>
        </form>
        </div>
  </div>
</section>
<!-- jQuery -->

<script src="<%=baseURL%>public/asset/plugins/bower_components/jquery/dist/jquery.min.js"></script>
