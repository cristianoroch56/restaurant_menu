<body>
<!-- Preloader -->
<div class="preloader">
  <div class="cssload-speeding-wheel"></div>
</div>
<section id="wrapper" class="login-register">
  <div class="login-box ">

      <% if (message != '') { %>
        <div class="alert alert-danger col-sm-12">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
          <strong>
          <%= message %></strong>
        </div>
<% } %>
  
    <div class="white-box">
      <form class="form-horizontal form-material" data-toggle="validator"  id="loginform" class="form" action="signup" method="POST"> 
        <h3 class="box-title m-b-20">Sign In</h3>  
        
        <div class="form-group m-t-40">
          <div class="col-xs-12">
            <input type="email" class="form-control"  name="email" id="inputEmail2" data-error="Bruh, that email address is invalid" placeholder="Username" required>
            <div class="help-block with-errors"></div>
          </div>
        </div>
        <div class="form-group">
          <div class="col-xs-12">
            <input class="form-control" type="password" data-toggle="validator" data-minlength="5" name="password" id="password" placeholder="Password" required>
            
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-12">
         
            <a href="javascript:void(0)" id="to-recover" class="text-dark pull-right"><i class="fa fa-lock m-r-5"></i> Forgot pwd?</a> </div>
        </div>
        <div class="form-group text-center m-t-20">
          <div class="col-xs-12">
            <button class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light" alt="alert" type="submit" id="sa-success">Log In</button>
          </div>
        </div>
        
        
      </form>
      <form class="form-horizontal" data-toggle="validator" id="recoverform" action="forgot_password" method="POST">
        <div class="form-group ">
          <div class="col-xs-12">
            <h3>Recover Password</h3>
            <p class="text-muted">Enter your Email and instructions will be sent to you! </p>
          </div>
        </div>
        <div class="form-group ">
          <div class="col-xs-12">
            <input type="email" class="form-control" id="inputEmail2" name="email" data-error="Bruh, that email address is invalid" placeholder="Email" required>
            <div class="help-block with-errors"></div>
          </div>
        </div>
        <div class="form-group text-center m-t-20">
          <div class="col-xs-12">
            <button class="btn btn-primary btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Reset</button>
            <a href="login" class="text-dark pull-right ">BACK TO LOGIN</a>
          </div>
        </div>
      </form>
    </div>
  </div>
</section>
<!-- jQuery -->
<script src="<%=baseURL%>public/asset/plugins/bower_components/jquery/dist/jquery.min.js"></script>
